        "# Define the path to your dataset in Google Drive\n",
        "dataset_path = '/content/drive/MyDrive/Cvpr/VOCdevkit/VOC2007'\n",
        "\n",
        "(train_dataset, info) = tfds.load(\"voc/2007\", split=\"train\", with_info=True, data_dir=dataset_path)\n",
