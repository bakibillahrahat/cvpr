      "cell_type": "code",
      "source": [
        "def compute_metrics(model, dataset):\n",
        "    detection_model = model\n",
        "\n",
        "    detection_metrics = metrics.DetectionMetrics(detection_model)\n",
        "    groundtruth_boxes = []\n",
        "    groundtruth_classes = []\n",
        "    detection_boxes = []\n",
        "    detection_scores = []\n",
        "    num_eval_steps = 0\n",
        "\n",
        "    for sample in dataset:\n",
        "        image = sample['image']\n",
        "        image = image[tf.newaxis, ...]\n",
        "\n",
        "        groundtruth_boxes.append(sample['objects']['bbox'].numpy())\n",
        "        groundtruth_classes.append(sample['objects']['label'].numpy())\n",
        "\n",
        "        predictions = detection_model.predict(image)\n",
        "\n",
        "        detection_boxes.append(predictions['detection_boxes'][0].numpy())\n",
        "        detection_scores.append(predictions['detection_scores'][0].numpy())\n",
        "\n",
        "        num_eval_steps += 1\n",
        "\n",
        "    groundtruth = {\n",
        "        'boxes': np.concatenate(groundtruth_boxes),\n",
        "        'classes': np.concatenate(groundtruth_classes)\n",
        "    }\n",
        "\n",
        "    detections = {\n",
        "        'boxes': np.concatenate(detection_boxes),\n",
        "        'scores': np.concatenate(detection_scores)\n",
        "    }\n",
        "\n",
        "    detection_metrics.update_state(groundtruth, detections)\n",
        "\n",
        "    return detection_metrics.result()\n",
        "\n",
        "# Evaluate the model on the test dataset\n",
        "evaluation_result = compute_metrics(faster_rcnn_model, test_dataset)\n",
        "\n",
        "# Print the evaluation result\n",
        "print(\"Evaluation Result:\")\n",
        "print(\"  mAP: {:.2f}%\".format(evaluation_result['DetectionMAPHistogram']))\n",
        "\n",
        "# Additional metrics can be printed as needed\n",
        "# For example, you can access precision, recall, etc. using evaluation_result['Precision/mAP@.50IOU']"
      ],
      "metadata": {
        "id": "alAPMCmqt0hO"
      },
      "execution_count": null,
      "outputs": []
    }
