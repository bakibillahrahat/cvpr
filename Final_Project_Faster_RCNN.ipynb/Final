    {
      "cell_type": "code",
      "source": [
        "def build_faster_rcnn_model():\n",
        "    # Load VGG16 as the backbone\n",
        "    vgg16_backbone = tf.keras.applications.VGG16(weights='imagenet', include_top=False, input_shape=(None, None, 3))\n",
        "\n",
        "    # Use TensorFlow Hub to add Faster R-CNN layers\n",
        "    faster_rcnn_module = hub.load(\"https://tfhub.dev/google/faster_rcnn/openimages_v4/inception_resnet_v2/1\")\n",
        "    faster_rcnn = faster_rcnn_module.signatures['default']\n",
        "\n",
        "    # Combine VGG16 backbone with Faster R-CNN\n",
        "    input_tensor = Input(shape=(None, None, 3))\n",
        "    x = vgg16_backbone(input_tensor)\n",
        "    x = Lambda(lambda image: tf.image.resize(image, (224, 224)))(x)  # Resize for compatibility\n",
        "    detections = faster_rcnn(image=x)\n",
        "\n",
        "    return Model(inputs=input_tensor, outputs=detections)"
      ],
      "metadata": {
        "id": "Rmo1qrOntcTT"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def post_process(predictions, confidence_threshold=0.5):\n",
        "    boxes = predictions['detection_boxes'][0].numpy()\n",
        "    scores = predictions['detection_scores'][0].numpy()\n",
        "    selected_boxes = boxes[scores >= confidence_threshold]\n",
        "\n",
        "    return selected_boxes"
